package FOR4;

public class Prime {
	public static void main(String[] args) {
		int enterTheNumber = 4;
		boolean flag = true;
		for(int i=2; i<enterTheNumber; i++) {
			if (enterTheNumber%2==0) {
				flag = false;
			}
			else {
				flag = true;
			}
		}
		if(flag) {
			System.out.println("it is prime");
		}
	}
}
